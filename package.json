  "bugs": {
    "url": "https://github.com/AlbertoChamorro/Webpack/issues"
  },
  "homepage": "https://github.com/AlbertoChamorro/Webpack#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "nib": "^1.1.2",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.23",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "rupture": "^0.7.1",
    "sass-loader": "^7.0.3",
    "style-loader": "0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
