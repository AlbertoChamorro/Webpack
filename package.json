{
  "name": "habilities-webpack",
  "version": "1.0.0",
  "description": "habilities for webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract-text": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi-entry-point": "webpack --config ./multi-entry-point/webpack.config.js",
    "build:dev": "webpack --config ./webpack-dev-server/webpack.config.js",
    "build:dev-watch": "webpack --config ./webpack-dev-server/webpack.config.js --watch",
    "build:dev-server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:videos": "webpack --config ./file-loader-videos/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbertoChamorro/Webpack.git"
  },
  "author": "@AlbertoChamorro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlbertoChamorro/Webpack/issues"
  },
  "homepage": "https://github.com/AlbertoChamorro/Webpack#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
  }
}
